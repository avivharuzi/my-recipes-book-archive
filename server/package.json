{
  "name": "my-recipes-book-server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest --passWithNoTests",
    "coverage": "jest --passWithNoTests --coverage",
    "start": "ts-node ./src/server.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon -w ./src --ext \".ts\" --exec npm start",
    "start:prod": "cross-env NODE_ENV=production npm run build && node dist/server.js"
  },
  "private": true,
  "dependencies": {
    "argon2": "^0.28.2",
    "aws-sdk": "^2.921.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "express-fileupload-validator": "^2.0.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mime-types": "^2.1.30",
    "mongoose": "^5.12.12",
    "mongoose-simple-pagination": "^1.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-fileupload": "^1.1.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/method-override": "0.0.31",
    "@types/mime-types": "^2.1.0",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/nodemailer": "^6.4.2",
    "@types/sharp": "^0.28.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
